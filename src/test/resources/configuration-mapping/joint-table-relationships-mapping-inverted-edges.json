              "FILM_ID"
            ],
            "toColumns": [
              "ACTOR_ID"
            ]
          },
          "direction": "inverse"
        },
        "properties": {
          "year": {
            "type": "DATE",
            "value": "$from.date",
            "mandatory": true,
            "readOnly": false,
            "notNull": false
          }
        }
      }
    }
  ]
}